{
  "name": "vuetage",
  "author": {
    "name": "vertocode | Everton Vanoni",
    "email": "evertonvanoni1@gmail.com",
    "url": "https://vertocode.com"
  },
  "title": "vuetage",
  "type": "module",
  "version": "0.0.58",
  "main": "./dist/vuetage.umd.js",
  "module": "./dist/vuetage.es.js",
  "license": "MIT",
  "types": "./dist/main.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "bash scripts/build.sh",
    "preview": "vite preview",
    "lint": "yarn eslint --ext .vue,.ts --ignore-pattern dist/ --ignore-pattern storybook-static/ .",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "exports": {
    ".": {
      "import": "./dist/vuetage.es.js",
      "require": "./dist/vuetage.umd.js"
    },
    "./dist/vuetage-ts.css": {
      "import": "./dist/vuetage.css",
      "require": "./dist/vuetage.css"
    }
  },
  "dependencies": {
    "font-awesome": "^4.7.0",
    "vue": "^3.3.7"
  },
  "devDependencies": {
    "@fortawesome/vue-fontawesome": "^3.0.3",
    "@storybook/addon-essentials": "^7.5.3",
    "@storybook/addon-interactions": "^7.5.3",
    "@storybook/addon-links": "^7.5.3",
    "@storybook/blocks": "^7.5.3",
    "@storybook/testing-library": "^0.2.2",
    "@storybook/vue3": "^7.5.3",
    "@storybook/vue3-vite": "^7.5.3",
    "@types/node": "^20.8.10",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vitest/coverage-v8": "^0.34.6",
    "@vue/test-utils": "^2.4.1",
    "eslint": "^8.51.0",
    "eslint-config": "^0.3.0",
    "eslint-plugin-storybook": "^0.6.15",
    "eslint-plugin-vue": "^9.17.0",
    "happy-dom": "^12.10.3",
    "jsdom": "^22.1.0",
    "path": "^0.12.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.5",
    "rollup-plugin-typescript2": "^0.36.0",
    "sass": "^1.52.2",
    "storybook": "^7.5.3",
    "typescript": "^4.9.5",
    "vite": "^4.5.0",
    "vite-plugin-dts": "^3.6.3",
    "vitest": "^0.34.6",
    "vue-fontawesome": "^0.0.2",
    "vue-tsc": "^0.34.7"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "keywords": [
    "vue",
    "vue3",
    "typescript",
    "vite",
    "component",
    "library"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/vertocode/vuetage"
  },
  "bugs": {
    "link": "https://github.com/vertocode/vuetage/issues",
    "contact": "evertonvanoni1@gmail.com"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  },
  "packageManager": "yarn@4.0.2"
}
